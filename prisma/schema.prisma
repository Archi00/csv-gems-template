generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cat_tmp_gems {
  ID               String   @id(map: "tmp-gems_pkey")
  Active           BigInt?
  Categories       String?
  Price            String?  @db.VarChar
  Reference        String?
  Quantity         BigInt?
  Name             String?
  Description      String?
  ShortDescription String?
  meta_title       String?  @map("meta-title")
  meta_description String?  @map("meta-description")
  imgUrls          String[]
  imgAlts          String[]

  @@map("cat-tmp-gems")
}

model en_tmp_gems {
  ID               String  @id
  Active           BigInt?
  Categories       String?
  Price            BigInt?
  Reference        String?
  Quantity         BigInt?
  Name             String?
  Description      String?
  ShortDescription String?
  meta_title       String? @map("meta-title")
  meta_description String? @map("meta-description")

  @@map("en-tmp-gems")
}

model es_tmp_gems {
  ID               String  @id
  Active           BigInt?
  Categories       String?
  Price            BigInt?
  Reference        String?
  Quantity         BigInt?
  Name             String?
  Description      String?
  ShortDescription String?
  meta_title       String? @map("meta-title")
  meta_description String? @map("meta-description")

  @@map("es-tmp-gems")
}
